{
  "plugins": [],
  "recurseDepth": 10,
  "opts": {
      "template": "./node_modules/daybrush-jsdoc-template",
      "destination": "./doc/"
  },
  "source": {
      "include": [
          "./src",
          "README.md",
          "node_modules/@egjs/component/src/component.js"
      ],
      "includePattern": "(.+\\.js(doc|x)?|.+\\.ts(doc|x)?)$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
          "jsdoc",
          "closure"
      ]
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
            "./static"
        ]
      }
    }
  },
  "docdash": {
      "menu": {
          "Github repo": {
              "href": "https://github.com/daybrush/page",
              "target": "_blank",
              "class": "menu-item",
              "id": "repository"
          }
      }
  }
}